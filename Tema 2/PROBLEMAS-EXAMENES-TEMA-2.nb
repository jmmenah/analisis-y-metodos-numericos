  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "+", "\[ImaginaryI]"}], ")"}], 
     RowBox[{"(", 
      RowBox[{"1", "+", "\[ImaginaryI]"}], ")"}]}], 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "-", "\[ImaginaryI]"}], ")"}], 
     RowBox[{"(", 
      RowBox[{"1", "+", "\[ImaginaryI]"}], ")"}]}]], TraditionalForm]],
  ExpressionUUID->"cc1f072a-ce44-4f84-a4d1-8d0467c80d28"],
