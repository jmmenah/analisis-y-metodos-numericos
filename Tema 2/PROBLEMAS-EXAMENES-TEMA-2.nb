Cell[TextData[{
 "Soluci\[OAcute]n:\nComenzamos calculando la fracci\[OAcute]n. Para ello \
multiplicamos y dividimos por el conjugado del denominador.\n",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"1", "+", 
     RowBox[{"3", "\[ImaginaryI]"}]}], 
    RowBox[{"1", "-", 
     RowBox[{"2", "\[ImaginaryI]"}]}]], TraditionalForm]],ExpressionUUID->
  "d40b6fa8-d9d6-43af-9996-2ab85b7b1dbb"],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"3", "\[ImaginaryI]"}]}], ")"}], 
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"2", "\[ImaginaryI]"}]}], ")"}]}], 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"2", "\[ImaginaryI]"}]}], ")"}], 
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"2", "\[ImaginaryI]"}]}], ")"}]}]], TraditionalForm]],
  ExpressionUUID->"2c05413b-163e-4484-8036-41d6259d16cb"],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"(", 
     RowBox[{"1", "+", 
      RowBox[{"5", "\[ImaginaryI]"}], "+", 
      RowBox[{"6", 
       SuperscriptBox["\[ImaginaryI]", "2"]}]}], ")"}], 
    RowBox[{"1", "+", "4"}]], TraditionalForm]],ExpressionUUID->
  "1f698c92-8c46-4512-bb58-4fd76bfaf991"],
 "=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"-", "5"}], "+", 
     RowBox[{"5", "\[ImaginaryI]"}]}], "5"], TraditionalForm]],ExpressionUUID->
  "0a580a44-3078-4f72-9b50-10f82442f375"],
 "=-1+\[ImaginaryI]   \[DoubleLongRightArrow]  ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     FractionBox[
      RowBox[{"1", "+", 
       RowBox[{"3", "\[ImaginaryI]"}]}], 
      RowBox[{"1", "-", 
       RowBox[{"2", "\[ImaginaryI]"}]}]], ")"}], "2"], TraditionalForm]],
  ExpressionUUID->"76504364-d221-417e-8b3f-167bf8156c82"],
 " = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "1"}], "+", "\[ImaginaryI]"}], ")"}], "2"], 
   TraditionalForm]],ExpressionUUID->"ff1e68e0-d5e7-4591-ae85-c952f502024a"],
 " = 1+",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[ImaginaryI]", "2"], TraditionalForm]],ExpressionUUID->
  "7d352e0f-0611-4e02-8b3b-3c807a972100"],
 "-2",
 Cell[BoxData[
  FormBox["\[ImaginaryI]", TraditionalForm]],ExpressionUUID->
  "afbd8123-af9f-4fe8-9c23-f5e883355218"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"-", "2"}], "\[ImaginaryI]"}], TraditionalForm]],ExpressionUUID->
  "6b95be2a-7372-44d8-824e-1ca0b967d196"],
 " \nRecordemos que la exponencial de un numero complejo es  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"a", "+", "b\[ImaginaryI]"}]], "=", " ", 
    RowBox[{
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"a", " "}]], "(", 
     RowBox[{
      RowBox[{"cos", " ", "b"}], " ", "+", 
      RowBox[{"\[ImaginaryI]", " ", "sen", " ", "b"}]}], ")"}]}], 
   TraditionalForm]],ExpressionUUID->"d1e9c627-c38a-4b7e-88fb-3e7d6adcd806"],
 ". Por tanto, ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     FractionBox["\[Pi]", "2"], "\[ImaginaryI]"}]], TraditionalForm]],
  ExpressionUUID->"79aedb60-4d74-4bb2-87df-fcf1e574cd0b"],
 "  = cos ",
 Cell[BoxData[
  FormBox[
   FractionBox["\[Pi]", "2"], TraditionalForm]],ExpressionUUID->
  "7e3b4e62-0968-4a9a-9ad5-61dbc2bb1913"],
 " +  \[ImaginaryI] sen ",
 Cell[BoxData[
  FormBox[
   FractionBox["\[Pi]", "2"], TraditionalForm]],ExpressionUUID->
  "ab4dea8f-8ae6-4f17-ae3f-0e7845a7b746"],
 " = \[ImaginaryI] .\n\nFinalmente,  ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     FractionBox["\[Pi]", "2"], "\[ImaginaryI]"}]], TraditionalForm]],
  ExpressionUUID->"05290113-d561-46f9-bc0c-8b610c867d97"],
 "  ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     FractionBox[
      RowBox[{"1", "+", 
       RowBox[{"3", "\[ImaginaryI]"}]}], 
      RowBox[{"1", "-", 
       RowBox[{"2", "\[ImaginaryI]"}]}]], ")"}], "2"], TraditionalForm]],
  ExpressionUUID->"fe8119af-ae67-427f-8641-daf0f2efddde"],
 "  = \[ImaginaryI] (-2\[ImaginaryI]) = - 2 ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[ImaginaryI]", "2"], TraditionalForm]],ExpressionUUID->
  "6b642d0a-6c91-4f82-8ca6-03b1c54201cf"],
 " = 2.\nCon el Mathematica,"
}], "Text",
