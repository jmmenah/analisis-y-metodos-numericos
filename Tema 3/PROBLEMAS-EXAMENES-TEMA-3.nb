  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"5", 
      SuperscriptBox["n", "2"]}], "+", 
     RowBox[{"2", "n"}], "-", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"5", 
        SuperscriptBox["n", "2"]}], "+", 
       RowBox[{"3", "n"}], "+", "4"}], ")"}]}], 
    RowBox[{
     RowBox[{"5", "n"}], "+", "2"}]], TraditionalForm]],ExpressionUUID->
  "ecf84993-02c7-4b3d-a64e-220d88ba1c51"],
 " = ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"-", "n"}], "-", "4"}], 
    RowBox[{
     RowBox[{"5", "n"}], "+", "2"}]], TraditionalForm]],ExpressionUUID->
  "60d8a151-d1a9-47a8-a245-101a6050b4dd"],
