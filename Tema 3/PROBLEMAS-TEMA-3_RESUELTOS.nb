Cell[TextData[{
 StyleBox["Soluci\[OAcute]n: ",
  FontWeight->"Bold"],
 "\nObservamos que la base tiende a 1 y el exponente tiende a infinito. Se \
tratar\[IAcute]a de un l\[IAcute]mite del n\[UAcute]mero \[ExponentialE].\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox[
       RowBox[{
        SuperscriptBox["n", "2"], " ", "+", " ", "n"}], 
       RowBox[{
        SuperscriptBox["n", "2"], " ", "-", "1"}]], ")"}], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["n", "2"], "+", "n"}], ")"}], "/", 
      RowBox[{"(", 
       RowBox[{"n", "+", "2"}], ")"}], " "}]], "}"}], TraditionalForm]],
  ExpressionUUID->"fd43099e-9920-4c33-a16e-0e962e83b3e1"],
 "=",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox[
       RowBox[{
        SuperscriptBox["n", "2"], " ", "-", "1", "+", "1", "+", " ", "n"}], 
       RowBox[{
        SuperscriptBox["n", "2"], " ", "-", "1"}]], ")"}], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["n", "2"], "+", "n"}], ")"}], "/", 
      RowBox[{"(", 
       RowBox[{"n", "+", "2"}], ")"}], " "}]], "}"}], TraditionalForm]],
  ExpressionUUID->"fabd5310-59de-4680-acd8-a120d0581925"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    SuperscriptBox[
