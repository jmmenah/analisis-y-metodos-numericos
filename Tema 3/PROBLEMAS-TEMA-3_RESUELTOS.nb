     RowBox[{
      RowBox[{"{", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          FractionBox[
           RowBox[{
            RowBox[{"5", 
             SuperscriptBox["n", "2"]}], "+", "2"}], 
