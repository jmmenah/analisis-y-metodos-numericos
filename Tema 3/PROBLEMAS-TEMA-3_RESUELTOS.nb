Cell[TextData[{
 "\n(1) ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         RowBox[{"5", 
