  FormBox[
   RowBox[{
    RowBox[{"|", 
     RowBox[{
      SuperscriptBox["2", "2"], "-", "4"}], "|"}], "=", "0"}], 
   TraditionalForm]],ExpressionUUID->"c507e1cc-46eb-4b74-8900-5386c1d8bef6"],
 ", f(3)=",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    RowBox[{
     SuperscriptBox["3", "2"], "-", "4"}], "|"}], TraditionalForm]],
  ExpressionUUID->"f203298d-13f6-4b67-ae2b-f44c59bc27f2"],
