 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"|", 
     RowBox[{
      SuperscriptBox["0", "2"], "-", "4"}], "|"}], "=", "4"}], 
   TraditionalForm]],ExpressionUUID->"78e6db28-cf1a-4583-b8e5-245f942892ce"],
