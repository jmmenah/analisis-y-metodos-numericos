       RowBox[{"17", "\[DoubleLongRightArrow]", "18"}], 
       SuperscriptBox["e", 
        RowBox[{
         RowBox[{"-", "2"}], "k"}]]}], "=", 
      RowBox[{
       RowBox[{"12", "\[DoubleLongRightArrow]", 
        SuperscriptBox["e", 
