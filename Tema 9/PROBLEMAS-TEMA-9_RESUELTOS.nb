     RowBox[{
      RowBox[{
       RowBox[{"23", "\[DoubleLongRightArrow]", "5"}], "-", "C"}], "=", 
      RowBox[{
       RowBox[{"23", "\[DoubleLongRightArrow]", "C"}], "=", 
       RowBox[{
