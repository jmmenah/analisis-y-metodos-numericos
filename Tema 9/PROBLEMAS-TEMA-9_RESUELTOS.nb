     RowBox[{
      RowBox[{"17", "\[DoubleLongRightArrow]", "5"}], "+", 
      RowBox[{"18", 
       SuperscriptBox["e", 
        RowBox[{
         RowBox[{"-", "k"}], "*", "2"}]]}]}], "=", 
     RowBox[{
