  FormBox[
   SuperscriptBox["k", "2"], TraditionalForm]],ExpressionUUID->
  "a381d432-8669-486b-8272-fba9bea38156"],
 "+",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"k", "+", "1"}], ")"}], "2"], TraditionalForm]],ExpressionUUID->
  "1e5f7a2d-19cc-4b7c-9be8-6ff0c7add324"],
 " = ",
 Cell[BoxData[
  FormBox[
   StyleBox[
    FractionBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{"k", "+", "1"}], ")"}], 
      RowBox[{"(", 
       RowBox[{"k", "+", "2"}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"2", "k"}], "+", "3"}], ")"}]}], "6"],
    FontSize->18], TraditionalForm]],ExpressionUUID->
  "c30aa53d-6036-42eb-b816-99e0033da7b0"],
 ",\n\t\tPartimos de ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["1", "2"], TraditionalForm]],ExpressionUUID->
  "8b4b892a-c639-4c85-823f-d9b290dd6fc6"],
