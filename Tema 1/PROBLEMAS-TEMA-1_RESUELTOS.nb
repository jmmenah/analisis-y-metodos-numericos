     RowBox[{"k", "+", "1"}], ")"}], "2"], TraditionalForm]],ExpressionUUID->
  "ed055027-8686-466e-8bf6-86e1014e1c98"],
 "=(k+1)",
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{
     RowBox[{"[", 
      RowBox[{
       FractionBox[
        RowBox[{"k", "(", 
         RowBox[{
          RowBox[{"2", "k"}], "+", "1"}], ")"}], "6"], "+", 
       RowBox[{"(", 
        RowBox[{"k", "+", "1"}], ")"}]}], "]"}], "="}],
    FontSize->18], TraditionalForm]],ExpressionUUID->
  "f50878a1-10fd-479d-b405-e38f32f76f35"],
 "(k+1)",
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{
     RowBox[{"[", 
      FractionBox[
       RowBox[{
        RowBox[{"2", 
         SuperscriptBox["k", "2"]}], "+", 
        RowBox[{"7", "k"}], "+", "6"}], "6"], "]"}], "="}],
    FontSize->18], TraditionalForm]],ExpressionUUID->
  "3a03cb67-924a-42ce-8de6-7ca5e57bca9d"],
 "(k+1)",
 Cell[BoxData[
  FormBox[
   StyleBox[
    RowBox[{
     RowBox[{"[", 
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{"k", "+", "2"}], ")"}], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", "k"}], "+", "3"}], ")"}]}], "6"], "]"}], "=", 
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{"k", "+", "1"}], ")"}], 
       RowBox[{"(", 
        RowBox[{"k", "+", "2"}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", "k"}], "+", "3"}], ")"}]}], "6"]}],
    FontSize->18], TraditionalForm]],ExpressionUUID->
  "b030e735-2e20-4a53-8051-9dffac558ae3"],
 "\n\t\t\t\t\n\t\tHemos comprobado que si se cumple para k, debe de cumplirse \
tambi\[EAcute]n para k+1 y por tanto, queda as\[IAcute] probada la inducci\
\[OAcute]n\n\t \t\t"
