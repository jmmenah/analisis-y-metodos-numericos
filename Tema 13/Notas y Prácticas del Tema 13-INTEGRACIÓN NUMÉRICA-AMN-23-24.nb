  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"f", "(", "3", ")"}], "+", 
     RowBox[{"f", "(", "5", ")"}]}], 
    FormBox["2",
     TraditionalForm]], TraditionalForm]],ExpressionUUID->
  "6bca5cd2-2673-4571-b9bd-6a574cf6f886"],
