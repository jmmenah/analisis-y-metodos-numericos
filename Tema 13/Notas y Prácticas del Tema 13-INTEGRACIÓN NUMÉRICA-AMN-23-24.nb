   FractionBox[
    RowBox[{
     RowBox[{"f", "(", "a", ")"}], "+", 
     RowBox[{"4", " ", 
      RowBox[{"f", "(", 
       FractionBox[
        RowBox[{"a", "+", "b"}], "2"], ")"}]}], "+", 
     RowBox[{"f", "(", "b", ")"}]}], "6"], TraditionalForm]],ExpressionUUID->
  "340af843-4d5b-44e5-b59c-ba2758b448e2"],
 " (b-a)\nEn este caso,\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", "1", "2"], 
    RowBox[{
     FractionBox["1", "x"], 
     RowBox[{"\[DifferentialD]", "x"}]}]}], TraditionalForm]],ExpressionUUID->
  "e85e1aa1-fd80-48fb-ad8b-daf304184585"],
