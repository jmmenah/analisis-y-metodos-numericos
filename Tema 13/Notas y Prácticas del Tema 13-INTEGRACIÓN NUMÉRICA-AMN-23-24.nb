    FormBox["6",
     TraditionalForm]], TraditionalForm]],ExpressionUUID->
  "85ac0bba-71b4-4204-9b2c-4868f5b3bb17"],
 " = (5-1) ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"f", "(", "1", ")"}], "+", 
     RowBox[{"4", 
      RowBox[{"f", "(", "3", ")"}]}], "+", 
     RowBox[{"f", "(", "5", ")"}]}], 
    FormBox["6",
     TraditionalForm]], TraditionalForm]],ExpressionUUID->
  "0c0634d0-ea14-4d3b-9bf7-1f64b4305196"],
 " = ",
