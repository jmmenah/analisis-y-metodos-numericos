  FormBox[
   FractionBox[
    RowBox[{
     RowBox[{"f", "(", "3", ")"}], "+", 
     RowBox[{"4", 
      RowBox[{"f", "(", 
       FormBox["4",
        TraditionalForm], ")"}]}], "+", 
     RowBox[{"f", "(", "5", ")"}]}], 
    FormBox["6",
     TraditionalForm]], TraditionalForm]],ExpressionUUID->
  "e4bdefe6-41ff-44c0-9b73-a2ed0638e61e"],
 "=  ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"1", "+", 
     RowBox[{"4", "/", "2"}], "+", 
     RowBox[{"1", "/", "3"}]}], 
